def prime(num):
    if num > 1:
        for i in range(2, num//2):
            if (num % i) == 0:
                c= 0
                break
            else:
                c= 1
    else:
        c= 0
    if c==0:
        print(num, "is a composite number")
    else:
        print(num, "is a prime number")
prime(11)
